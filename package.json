{
  "name": "lessgo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "tsc --pretty --noEmit",
    "lint": "next lint",
    "prepare": "husky install",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "format": "prettier --write .",
    "test-all": "npm run check-types && npm run check-format && npm run check-lint && npm run build"
  },
  "dependencies": {
    "chance": "^1.1.11",
    "classnames": "^2.3.2",
    "next": "14.0.1",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^4.12.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.0",
    "@types/chance": "^1.1.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "autoprefixer": "^10.0.1",
    "daisyui": "^3.9.4",
    "eslint": "^8.53.0",
    "eslint-config-next": "14.0.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^1.0.1",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "postcss": "^8",
    "prettier": "3.0.3",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.2.2"
  }
}
